#include <iostream>
using namespace std;

int main() {

    int num1;
    int num2;

    int sum;

    cout << "Please Enter First Number : " << endl;
    cin >> num1;


    cout << "Please Enter Second Number : " << endl;
    cin >> num2;

    sum = num1 + num2;

    cout << "Total For The Number Is : " << sum << endl;


    return 0;
}